{
  "Name": "SMG2Achievements",
  "Author": "AwesomeTMC, Evanbowl, Lord-G",
  "Description": "An API to allow other modules to check if conditions specified here are True or False.",
  "APIId": "SMG2Achievementsg",
  "ModuleExtensionDefinition": [
    {
      "Name": "SuperFlagCustomFlags",
      "CodeGenSource": "GEN_SuperFlag.h",
      "CodeGenDestination": "include/SuperFlag.h",
      "Variables": [
        "Name",
        "FunctionName"
      ],
      "CodeGenData": [
        {
          "ReplaceTargetName": "CustomFlagConditionals",
          "ReplaceFormatData": "{{ \"{0}\", {1} }}"
        },
        {
          "ReplaceTargetName": "Externs",
          "ReplaceFormatData": "extern bool {1}(JMapInfoIter iter);"
        }
      ]
    }
  ],
  "ModuleDependancies": [
    "PTUtils",
    "Syati_ExtGameData_API",
    "Syati_Common_Functions"
  ],
  "InstallDependencies": [
    "PTUtils",
    "Syati_Init",
    "Syati_ExtGameData_API",
    "Syati_Common_Functions"
  ],
  "BuildTasks": [{
    "Task": "Copy",
    "From": "disc/",
    "To": "../../Output/disc/"
  }],
  "ModuleData": [
    {
      "NewSaveChunk": [
        {
          "ChunkMemberType": "AchievementChunk",
          "ChunkMemberName": "mAchievementChunk",
          "ChunkCreationFunc": "createBinaryChunk<AchievementChunk>",
          "Include": "Achievements.h"
        }
      ],
      "OnGameSceneStart": [
        {
          "Name": "onSceneStart",
          "Function": "onSceneStart",
          "Include": "Hooks.h"
        }
      ]
    }
  ]
}